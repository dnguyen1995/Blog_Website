<%- include("header") -%>
<form action="/" method="post">
  <h2 class="compose-heading">Title</h2>
  <input class="text-field" type="text" name="blogTitle" placeholder="Your title here" autocomplete="off">
  <h2 class="compose-heading">Post</h2>
  <input class="text-field text-blog" type="textarea" name="blogText" autocomplete="off">
  <button class="submit-button" type="submit" name="postSubmit">Submit Post</button>
</form>
<%- include("footer") -%>
